{
  "version": 2,
  "routes": [
    {
      "src": "/rss.xml",
      "headers": {
        "content-type": "application/rss+xml; charset=UTF-8"
      }
    },
    {
      "src": "/rss",
      "headers": {
        "content-type": "application/rss+xml; charset=UTF-8"
      },
      "dest": "/rss.xml"
    },
    {
      "src": "/sitemap.xml",
      "headers": {
        "content-type": "application/xml; charset=UTF-8"
      },
      "dest": "/sitemap-index.xml"
    },
    {
      "src": "/sitemap",
      "headers": {
        "content-type": "application/xml; charset=UTF-8"
      },
      "dest": "/sitemap-index.xml"
    },
    {
      "src": "/blog/(.*)",
      "status": 301,
      "headers": {
        "Location": "/$1"
      }
    }
  ],
  "github": {
    "silent": true
  }
}
